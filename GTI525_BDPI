@startuml
Title BD: Gestion des salles.
class Mobile {
(CléP) id : Int[11] *AutoInc, Unique*
token : VarChar[40] *Unique*
ipAddress : Varchar[30] *NonNull*
loginTime : DateTime
logoutTime : DateTime
status : Varchar[30]
}

class MobileCommLog {
(CléP) id : Int[11] *AutoI*
httpResponse : Int[3]
mobileid_id : Int[11]
ticketid_id : Int[11]
}

class Terminal {
(CléP) id : Int[11] *AutoI, Unique*
status : VarChar[30]
ipAddress : Varchar[30] *NonNull*
}

class Ticket {
(CléP) id : Int[11] *AutoI, Unique*
detenteur : VarChar[120]
ticketHash : VarChar[40] *Unique*
validationTime : DateTime
validationTerminal_id : Lié a Mobile:id Int[11]
owner : VarChar[120]
status : VarChar[20]
event_id : Lié a event:id Int[11]
}

class Auditorium {
(CléP) id : Int[11] *AutoI, Unique*
address : VarChar[120]
name : VarChar[120]
}

class Event {
(CléP) id : Int[11] *AutoI, Unique*
time : DateTime
name : VarChar[120]
auditorium_id : Lié a auditorium:id Int[11]
}

class APIKey {
(CléP) id : Int[11] *AutoI, Unique*
created : DateTime
modified : DateTime
name : VarChar[120]
key : VarChar[64] *Unique*
}

@enduml